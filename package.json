{
  "name": "@bowtie/api",
  "version": "0.1.4",
  "description": "A simple class to standardize API interactions",
  "main": "index.js",
  "engines": {
    "node": ">=6.x"
  },
  "scripts": {
    "build": "babel src -d lib --presets env",
    "test": "npm run build && mocha test/**/*.spec.js",
    "test:watch": "mocha test/**/*.spec.js --watch",
    "preversion": "npm test",
    "postversion": "git push --follow-tags"
  },
  "keywords": [
    "bowtie",
    "api",
    "base",
    "fetch",
    "async",
    "get",
    "post",
    "put",
    "delete",
    "rest"
  ],
  "author": "Bowtie",
  "license": "MIT",
  "dependencies": {
    "@bowtie/utils": "^0.1.6"
  },
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "mocha": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bowtie-co/node-api.git"
  },
  "bugs": {
    "url": "https://github.com/bowtie-co/node-api/issues"
  },
  "homepage": "https://github.com/bowtie-co/node-api#readme"
}
